{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "utils/*": ["./src/utils/*"],
      "commonDefines": ["./src/defines/common.d.ts"],
      "defines/*": ["./src/defines/*"],
      "UI/*": ["./src/UI/*"],
      "fetch": ["./src/services/fetch/index"],
      "helpers": ["./src/utils/helpers"],
    } 
  },
  "include": ["./src"]
}
